<%- include('./common/head'); %>
<header>
    <%- include('./common/navbar', {user: user}); %>
     <script
      type="text/javascript"
      src="https://www.gstatic.com/charts/loader.js"
    ></script>
    <script type="text/javascript">
      google.charts.load("current", {
        packages: ["map"],

        mapsApiKey: "<%= process.env.GOOGLE_API_KEY %>",
      });
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ["Lat", "Long", "Name"],
          [59.6567, 16.6709, "patient1"],
          [57.3365, 12.5164, "patient2"],
        ]);
        var options = {
          showTooltip: true,
          showInfoWindow: true,
        };
        var map = new google.visualization.Map(
          document.getElementById("map_div")
        );
        map.draw(data, {
          showTooltip: true,
          showInfoWindow: true,
        });
      }
    </script>
</header>
<body>
    <%- include('./common/map', {latitudes: []}); %>
    <%- include('./common/patient-data', {patients: patients}); %>
    <%- include('./common/rss-feed', {feed: feed}); %>

</body>